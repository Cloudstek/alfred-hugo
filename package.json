{
  "name": "hugo",
  "version": "1.0.0",
  "description": "Alfred workflow bindings for NodeJS",
  "author": "Maarten de Boer <maarten@cloudstek.nl>",
  "main": "index.js",
  "scripts": {
    "test": "xo && flow && ava",
    "build": "xo && flow && babel src --out-dir .",
    "watch": "babel -w src --out-dir ."
  },
  "keywords": [
    "alfred",
    "workflow",
    "macos",
    "mac",
    "osx",
    "util",
    "utility",
    "helper"
  ],
  "license": "BSD-2-Clause",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "flow-bin": "^0.40.0",
    "gulp": "^3.9.1",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "cache-conf": "^0.5.0",
    "conf": "^0.12.0"
  },
  "ava": {
    "babel": "inherit"
  },
  "xo": {
    "envs": ["node"],
    "space": 4,
    "parser": "babel-eslint",
    "ignores": [
      "*.js"
    ]
  }
}
